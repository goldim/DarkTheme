{
  "$schema": "https://qooxdoo.org/schema/Manifest-2-0-0.json",
  "info": {
    "name": "DarkTheme",
    "summary": "Dark Theme Contribution",
    "description": "A dark theme for qx 6.0 and greater.",
    "homepage": "https://github.com/goldim/DarkTheme",
    "license": "MIT",
    "authors": [
      {
        "name": "scro34",
        "email": "schroeder@scro34.de"
      }
    ],
    "version": "2.0.0"
  },
  "provides": {
    "namespace": "darktheme",
    "encoding": "utf-8",
    "class": "source/class",
    "resource": "source/resource",
    "translation": "source/translation",
    "application": {
      "class": "widgetbrowser.Application",
      "theme": "darktheme.theme.Theme",
      "name": "widgetbrowser",
      "title": "WidgetBrowser",
      "include": [
        "qx.theme.Simple",
        "qx.theme.Classic",
        "widgetbrowser.pages.Tree",
        "widgetbrowser.pages.List",
        "widgetbrowser.pages.Table",
        "widgetbrowser.pages.Form",
        "widgetbrowser.pages.ToolBar",
        "widgetbrowser.pages.Window",
        "widgetbrowser.pages.Tab",
        "widgetbrowser.pages.Control",
        "widgetbrowser.pages.EmbedFrame",
        "widgetbrowser.pages.Basic",
        "widgetbrowser.pages.Misc"
      ],
      "exclude": [
        "qx.theme.Modern"
      ]
    }
  },
  "requires": {
    "@qooxdoo/framework": "^7.0.0",
    "qooxdoo/qxl.formdemo": "^2.0.0",
    "qooxdoo/qxl.versionlabel": "^2.0.0"
  }
}